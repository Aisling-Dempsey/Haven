{{#results}}
     <div class="row query-result">
        <div class="col-l-9 col-xs-12">
            <div class="row">
                <div class="col-l-3 col-xs-3 bus-search-photo">
                    {{#photo}}
                    <img src="{{ photo }}">
                    {{/photo}}
                </div>
                <div class="col-l-9 col-xs-9 bus-search-text">
                    <div class="row">
                        <div class="col-l-9 col-xs-9 bus-search-info">
                            <div class="row">
                                <a class="bus-link" href="/info/{{bus-id}}.json">
                                    <h3 class="bus-name">{{ name }}</h3>
                                </a>
                            </div>
                            <div class="row">
                                {{#phone}}
                                    <p class="bus-phone">{{ phone }}</p>
                                {{/phone}}
                            </div>
                            <div class="row">
                                {{#address1}}
                                    <p class="bus-address-1">{{address1}}</p>
                                {{/address1}}
                            </div>
                            <div class="row">
                                {{#address2}}
                                    <p class="bus-address-2">{{adress2}}</p>
                                {{/address2}}
                            </div>
                            <div class="row">
                                {{#url}}
                                    <a href="{{url}}">Website</a>
                                {{/url}}
                            </div>
                            <div class="row">
                                <p class="business-categories">
                                    {{#category}}
                                        {{category}}
                                    {{/category}}
                                </p>
                            </div>
                        </div>
                        <div class="col-l-3 col-xs-3 bus-search-ratings">
                            <div class="row">
                                {{#haven-score}}
                                    <p class="bus-haven-score"> Haven Score:{{haven-score}} out of 5 </p>
                                {{/haven-score}}
                                {{^haven-score}}
                                    <p class="bus-haven-score">Nobody has rated this business yet. Be the first!</p>
                                {{/haven-score}}
                            </div>
                            <div class="row">
                                <p>Yelp score: {{yelp-score}}</p>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
{{/results}}